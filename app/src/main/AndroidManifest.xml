<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="zeng.fanda.com.rxjava2demo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".BooleanObservableActivity" />
        <activity android:name=".CreateObservableActivity" />
        <activity android:name=".TransforObservableActivity" />
        <activity android:name=".FilterObservableActivity" />
        <activity android:name=".CombineObservableActivity" />
        <activity android:name=".ErrorHandleObservableActivity" />
        <activity android:name=".HelpObservableActivity" />
        <activity android:name=".ConnectObservableActivity" />
        <activity android:name=".BackPressureActivity" />
        <activity android:name=".ComposeActivity" />
        <activity android:name=".ParallelActivity" />
        <activity android:name=".rxbinding.RxBindingActivity" />
        <activity android:name=".SubjectActivity" />
    </application>

</manifest>